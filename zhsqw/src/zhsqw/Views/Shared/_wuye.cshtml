<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/easyui/themes/default/easyui.css">
    <link rel="stylesheet" href="~/easyui/themes/icon.css">
    @RenderSection("styles", required: false)
    <style type="text/css">
        body{font-size:12px}
        table{border-right:1px solid #ffcccc;border-bottom:1px solid #ffcccc;margin:5px auto 0px;}
        th{border-left:1px solid #ffcccc;border-top:1px solid #ffcccc;background:#d4ecfc;height:25px;}
        td{border-left:1px solid #ffcccc;border-top:1px solid #ffcccc;height:25px;text-align:center;}
        .qtitle{background:#eaf2ff;}
        .qcontent{padding-left:3px;text-align:left;}
    </style>
    <script src="~/easyui/jquery.min.js"></script>
    <script src="~/easyui/jquery.easyui.min.js"></script>
    @Html.ApplicationInsightsJavaScript(TelemetryConfiguration)
</head>
<body>
    <div>
        @RenderBody()
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
