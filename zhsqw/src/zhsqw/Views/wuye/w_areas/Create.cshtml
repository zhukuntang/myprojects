@model w_areas

@{
    Layout = "_wuye";
    ViewData["Title"] = "添加社区";
}

<form asp-action="Create">
    <div style="border-bottom:1px solid #ffcccc;width:700px;height:25px;font-size:15px;font-weight:bold;text-align:center;margin:10px auto">添加社区</div>
    <table cellpadding="0" cellspacing="0" style="width:700px">
        <tr>
            <td style="width:20%" class="qtitle">所在地区：</td>
            <td style="width:80%" class="qcontent">
                <input asp-for="CityID" style="width:200px" />
            </td>
        </tr>
        <tr>
            <td style="width:20%" class="qtitle">社区名称：</td>
            <td style="width:80%" class="qcontent">
                <input asp-for="AreaName" style="width:200px" />
            </td>
        </tr>
        <tr>
            <td style="width:20%" class="qtitle">详细地址：</td>
            <td style="width:80%" class="qcontent">
                <input asp-for="Addr" style="width:500px" />
            </td>
        </tr>
        <tr>
            <td style="height:35px" colspan="2">
                <a onclick="$('form').submit();$(this).parent().text('正在提交...')" class="easyui-linkbutton" data-options="iconCls:'icon-ok'">添加提交</a>
                <a asp-action="Index" class="easyui-linkbutton" data-options="iconCls:'icon-redo'">返回列表</a>
            </td>
        </tr>
</table>
</form>